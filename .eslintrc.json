{
  "extends": [
    "airbnb-base",
    "plugin:jsx-a11y/recommended",
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "globals": {
    "window": true,
    "document": true
  },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "plugins": [
    "jsx-a11y",
    "prettier",
    "vue"
  ],
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
  }
}
