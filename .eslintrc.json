{
    "extends": ["airbnb-base"],
    "globals": {
        "window": true,
        "document": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "class-methods-use-this": 0,
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": true
            }
        ],
        "indent": ["error", 4],
        "keyword-spacing": 0,
        "no-unused-vars": 0
    }
}
