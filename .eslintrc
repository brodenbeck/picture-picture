{
    parser: "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": 0,
        "indent": ["error", 4],
        "keyword-spacing": 0,
        "class-methods-use-this": 0
    },
    "extends": ["airbnb-base"],
    "globals": {
        "window": true,
        "document": true
    }
}